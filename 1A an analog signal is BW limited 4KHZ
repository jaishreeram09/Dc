fm=4000; //modulating frequency
r=2*fm;
disp(r); //Message rate
p1=1/8; //probability
p2=1/8; //probability
p3=3/8; //probability
p4=3/8; //probability
I1=log2(1/p1); //Information
I2=log2(1/p2);
I3=log2(1/p3);
I4=log2(1/p4);
disp(I1);
disp(I2);
disp(I3);
disp(I4);
H=I1*p1+I2*p2+I3*p3+I4*p4;
disp(H); //Entropy
R=r*H;
disp(R);
